part1,part2,part3: part1.o,part2.o,part3.o
	gcc -Wall -std=c99 -o part1,part2,part3 part1.o,part2.o,part3.o -lm

part1.o,part2.o,part3.o: part1.c,part2.part3.c 
	gcc -Wall -std=c99 -c part1.c,part2.c,part3.c

clean:
	-rm -f *.o part1,part2,part3 core
tar:
	tar cvf submit.tar mention.c Makefile README testcases.txt
